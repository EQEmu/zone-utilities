{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) azone",
            "type": "cppdbg",
            "request": "launch",
            "program": "/src/build/bin/azone",
            "args": ["cryptofshade"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/build/bin",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}